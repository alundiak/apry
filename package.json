{
  "name": "@lundiak/react-sum",
  "version": "1.3.0",
  "private": true,
  "description": "Experimental ReactJS component to show sum of 2 numbers.",
  "browser": "dist/app.js",
  "module": "src/App.jsx",
  "main": "dist/app.js",
  "esm": "src/App.jsx",
  "jsnext:main": "src/App.jsx",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "yarn build && yarn build-storybook && cp -r storybook-static ./build/storybook",
    "deploy": "gh-pages -d build",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "bundledDependencies": false,
  "dependencies": {
    "ascii-math": "^2.0.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-docs": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-info": "^6.0.0-alpha.2",
    "@storybook/addon-knobs": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addon-notes": "^6.0.0-alpha.6",
    "@storybook/addons": "^6.2.9",
    "@storybook/builder-webpack4": "^6.2.9",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.9",
    "@storybook/theming": "^6.2.9",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "babel-loader": "8.1.0",
    "gh-pages": "^3.1.0",
    "jest-cucumber": "^3.0.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "testMatch": [
      "**/*.test.js",
      "**/*.test.jsx",
      "**/*.steps.js",
      "**/*.steps.jsx"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alundiak/react-sum.git"
  },
  "bugs": {
    "url": "https://github.com/alundiak/react-sum/issues"
  },
  "homepage": "https://alundiak.github.io/react-sum",
  "author": "Andrii Lundiak",
  "license": "ISC"
}